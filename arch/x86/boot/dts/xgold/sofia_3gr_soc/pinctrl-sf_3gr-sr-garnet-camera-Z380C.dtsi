cam1_default_pins:cam1_default_pins {
	pinctrl-single,pins = <
		0x314 0x1500	/* cam1_pd */
		0x330 0x1500	/* SOC_VCM_EN */
		0x2f8 0x100	/* cam1_1_2V_en */
	>;
};
cam1_inactive_pins:cam1_inactive_pins {
	pinctrl-single,pins = <
		0x314 0x2100	/* cam1_pd */
		0x330 0x2100	/* SOC_VCM_EN */
		0x2f8 0x2100	/* cam1_1_2V_en */
	>;
};
cam1_sleep_pins:cam1_sleep_pins {
	pinctrl-single,pins = <
		0x314 0x1500	/* cam1_pd */
		0x330 0x1500	/* SOC_VCM_EN */
		0x2f8 0x100	/* cam1_1_2V_en */
	>;
};
cam2_default_pins:cam2_default_pins {
	pinctrl-single,pins = <
		0x33c 0x100	/* cam2_pd */
	>;
};
cam2_inactive_pins:cam2_inactive_pins {
	pinctrl-single,pins = <
		0x33c 0x2100	/* cam2_pd */
	>;
};
cam2_sleep_pins:cam2_sleep_pins {
	pinctrl-single,pins = <
		0x33c 0x100	/* cam2_pd */
	>;
