i2c1_default_pins:i2c1_default_pins {
	pinctrl-single,pins = <
		0x350 0x10011	/* i2c1_scl */
		0x354 0x10011	/* i2c1_sda */
		0x3a4 0x1010 /*mclk*/
	>;
};
i2c1_inactive_pins:i2c1_inactive_pins {
