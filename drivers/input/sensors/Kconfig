#
# Sensor drivers configuration
#

config SENSORS_CM36686
        bool "CM36686 Proximity & Light Sensor"
        depends on I2C

config SENSORS_AL3320A
        bool "AL3320A Light Sensor"
        depends on I2C

config HALL_SENSOR
       bool "Hall sensor"
       default n
       help
          Say Y here if you want enable hall sensor driver
          If unsure, say N.

config SENSORS_AP3425
        bool "AP3425 Proximity & Light Sensor"
        depends on I2C
